{
  "name": "sportradenenzo",
  "version": "0.3.0",
  "description": "Poulewebsite voor de beste poulegroep",
  "repository": {
    "url": "https://github.com/RensReus/Sportradenenzo"
  },
  "dependencies": {
    "async": "^3.2.0",
    "babel-core": "^6.26.3",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.19.0",
    "canvasjs": "^1.8.3",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "flash": "^1.1.0",
    "fs-extra": "^9.0.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.2",
    "morgan": "^1.10.0",
    "node-schedule": "^1.3.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3",
    "pg-pool": "^3.2.1",
    "request": "^2.88.2",
    "sql.js": "^1.3.0",
    "textarea-caret": "^3.1.0",
    "tslib": "^2.0.1"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "start-dev": "concurrently \"npm run server-dev\" \"npm run client\"",
    "server": "node build/server.js",
    "server-dev": "nodemon --config \"nodemon.json\"/",
    "client": "node start-client.js",
    "dev": "echo \"This command has been deprecated. Use 'npm start'\" && exit 1",
    "lint": "eslint .",
    "build": "npm i --only=dev && npx tsc && cd src/client/ && npm i --only=dev && npm i --save @babel/runtime && npm run build && cd ../..  && node heroku-build.js"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/node": "^14.6.3",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.2"
  }
}
